{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "itemInfo": {
      "type": "component",
      "repeatable": false,
      "component": "item-info.item-info",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "items"
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "isBestSeller": {
      "type": "boolean",
      "default": false
    }
  }
}
